import pandas as pd
import numpy as np
import os
import matplotlib.pyplot as plt
import seaborn as sns
from sklearn.decomposition import PCA
from sklearn.preprocessing import scale
50/2: ski_data = pd.read_csv('data/ski_data_cleaned.csv')
50/3: os.getcwd()
