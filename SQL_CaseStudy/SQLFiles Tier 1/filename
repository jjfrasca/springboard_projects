SELECT
    DISTINCT facility,
    month,
    COUNT(*) AS usage_per_month
FROM (
    SELECT
        name AS facility,
        strftime('%m', starttime) AS month
    FROM Facilities AS f
    JOIN Bookings AS b
    USING (facid)
    WHERE memid > 0) AS sub
GROUP BY facility

;
        """
    cur.execute(query1)
 
    rows = cur.fetchall()
 
    for row in rows:
        print(row)


