        print(sqlite3.version)
    except Error as e:
        print(e)
 
    return conn
 
def select_all_tasks(conn):
    """
    Query all rows in the tasks table
    :param conn: the Connection object
    :return:
    """
    cur = conn.cursor()
    
    query1 = """

SELECT surname, firstname, recommendedby
FROM (
    SELECT 
        memid,
        surname,
        firstname
    FROM Members) AS m1
JOIN Members AS m2
ON m1.memid = m2.recommendedby;
        """
    cur.execute(query1)
 
    rows = cur.fetchall()
 
