    """
    Query all rows in the tasks table
    :param conn: the Connection object
    :return:
    """
    cur = conn.cursor()
    
    query1 = """

SELECT m1.surname, m1.firstname, recommendedby
FROM (
    SELECT 
        memid,
        surname,
        firstname
    FROM Members) AS m1
JOIN Members AS m2
ON m1.memid = m2.recommendedby;
        """
    cur.execute(query1)
 
