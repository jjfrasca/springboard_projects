def create_price_ratio(d):
    df_bfilter = df.filter(df[df['London_Borough']==b])
    price_00 = float(d['Average_price'][d['Year']==2000])
    price_20 = float(d['Average_price'][d['Year']==2020])
    price_ratio = price_00/price_20
    return price_ratio
