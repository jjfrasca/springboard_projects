    """
    Query all rows in the tasks table
    :param conn: the Connection object
    :return:
    """
    cur = conn.cursor()
    
    query1 = """

SELECT
    facility,
    month,
    COUNT(*)
FROM (
    SELECT
        name AS facility,
        strftime('%m', starttime) AS month,
        bookid
    FROM Facilities AS f
    JOIN Bookings AS b
    USING (facid)
    WHERE memid > 0) AS sub
GROUP BY facility, month;
