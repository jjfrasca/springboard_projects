    """
    Query all rows in the tasks table
    :param conn: the Connection object
    :return:
    """
    cur = conn.cursor()
    
    query1 = """
        WITH full_name AS (
            SELECT
                (firstname ||''|| surname)
                FROM Members)

        SELECT *
        FROM full_name
        """
    cur.execute(query1)
 
    rows = cur.fetchall()
 
    for row in rows:
        print(row)


