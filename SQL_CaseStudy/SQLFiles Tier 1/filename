    """
    Query all rows in the tasks table
    :param conn: the Connection object
    :return:
    """
    cur = conn.cursor()
    
    query1 = """
WITH g AS (
    SELECT facid, name AS facility_name, guestcost * slots AS revenue
    FROM Bookings
    JOIN Facilities
    USING (facid)
    WHERE memid = 0),
    
m AS (
    SELECT facid, name AS facility_name, membercost * slots AS revenue
    FROM Bookings
    JOIN Facilities
    USING (facid)
    WHERE memid > 0
)

SELECT
    g.revenue,
    m.revenue,
    g.revenue + m.revenue AS total_revenue
    
FROM g
JOIN m
USING (facid);
        """
    cur.execute(query1)
 
    rows = cur.fetchall()
 
    for row in rows:
        print(row)


