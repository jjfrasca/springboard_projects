def create_price_ratio(b):
    df_bfilter = df.filter(df[df['London_Borough']==b])
    price_00 = df_bfilter['Average_price']['Year'['2000']]
    price_20 = df_bfilter['Average_price']['Year'['2020']]
    price_ratio = price_00/price_20
    return price_ratio
