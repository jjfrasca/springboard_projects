        print(sqlite3.version)
    except Error as e:
        print(e)
 
    return conn
 
def select_all_tasks(conn):
    """
    Query all rows in the tasks table
    :param conn: the Connection object
    :return:
    """
    cur = conn.cursor()
    
    query1 = """

SELECT facid, name AS facility_name, SUM(guestcost), SUM(slots)
    FROM Bookings
    JOIN Facilities
    USING (facid)
    WHERE memid = 0;
        """
    cur.execute(query1)
 
    rows = cur.fetchall()
 
    for row in rows:
